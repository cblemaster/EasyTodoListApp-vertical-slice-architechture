<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--colors-->
    <Color x:Key="PrimaryDark">#104861</Color>
    <Color x:Key="PrimaryLight">#215C98</Color>
    <Color x:Key="SecondaryDark">#12501A</Color>
    <Color x:Key="SecondaryLight">#3C7D22</Color>
    <Color x:Key="TertiaryDark">#7E350E</Color>
    <Color x:Key="TertiaryLight">#F1A983</Color>
    <Color x:Key="GhostWhiteAccent">GhostWhite</Color>
    <Color x:Key="NavajoWhiteAccent">NavajoWhite</Color>
    <Color x:Key="PaleGreenAccent">PaleGreen</Color>
    <Color x:Key="PaleYellowAccent">#FFFFEB</Color>
    
    <!--brushes-->
    <SolidColorBrush Color="{StaticResource PrimaryDark}" x:Key="PrimaryDarkBrush"/>
    <SolidColorBrush Color="{StaticResource PrimaryLight}" x:Key="PrimaryLightBrush"/>
    <SolidColorBrush Color="{StaticResource SecondaryDark}" x:Key="SecondaryDarkBrush"/>
    <SolidColorBrush Color="{StaticResource SecondaryLight}" x:Key="SecondaryLightBrush"/>
    <SolidColorBrush Color="{StaticResource TertiaryDark}" x:Key="TertiaryDarkBrush"/>
    <SolidColorBrush Color="{StaticResource TertiaryLight}" x:Key="TertiaryLightBrush"/>
    <SolidColorBrush Color="{StaticResource GhostWhiteAccent}" x:Key="GhostWhiteAccentBrush"/>
    <SolidColorBrush Color="{StaticResource NavajoWhiteAccent}" x:Key="NavajoWhiteAccentBrush"/>
    <SolidColorBrush Color="{StaticResource PaleYellowAccent}" x:Key="PaleYellowAccentBrush"/>
    <SolidColorBrush Color="PaleGreen" x:Key="PaleGreenAccentBrush"/>
</ResourceDictionary>
