<Application x:Class="EasyTodoListApp.Desktop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"             
             StartupUri="pack://application:,,,/UseCases/GetTodos/GetTodosWindow.xaml"
             ThemeMode="System">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/Colors.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/Styles.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/Templates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
